{
   "name": "@scrypted/lifx",
   "version": "0.1.3",
   "description": "A Lifx plugin for Scrypted",
   "author": "Scrypted",
   "license": "Apache",
   "scripts": {
      "prepublish": "NODE_ENV=production scrypted-webpack",
      "prescrypted-vscode-launch": "scrypted-webpack",
      "scrypted-vscode-launch": "scrypted-deploy-debug",
      "scrypted-deploy-debug": "scrypted-deploy-debug",
      "scrypted-debug": "scrypted-debug",
      "scrypted-deploy": "scrypted-deploy",
      "scrypted-webpack": "scrypted-webpack"
   },
   "keywords": [
      "lifx",
      "scrypted",
      "plugin"
   ],
   "scrypted": {
      "name": "LIFX Controller",
      "type": "Device",
      "interfaces": ["DeviceProvider"]
   },
   "devDependencies": {
      "scrypted-deploy": "^1.2.3",
      "webpack-merge": "^4.2.1"
   },
   "dependencies": {
      "axios": "^0.18.0",
      "node-lifx": "github:koush/node-lifx"
   }
}
